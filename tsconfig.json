{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist",
    "strict": true,
    "target": "es5",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "types": [
      "node"
    ]
  },
  "include": [
    "src"
  ]
}
